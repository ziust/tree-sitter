{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ziust",
      "camelcase": "Ziust",
      "title": "Ziust",
      "scope": "source.ziust",
      "file-types": [
        "z"
      ],
      "injection-regex": "^ziust$",
      "class-name": "TreeSitterZiust"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Ziust grammar for tree-sitter",
    "authors": [
      {
        "name": "Juyeong Maing",
        "email": "mjy9088@naver.com",
        "url": "https://github.com/mjy9088"
      }
    ],
    "links": {
      "repository": "https://github.com/ziust/tree-sitter.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}